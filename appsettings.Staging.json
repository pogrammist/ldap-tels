{
	"ConnectionStrings": {
		"DefaultConnection": "Data Source=ldap_tels.stage.db"
	},
	"LdapSync": {
		"IntervalMinutes": 30,
		"Enabled": true
	},
	"LdapSettings": {
		"Server": "stage-ldap.example.com",
		"Port": 389,
		"UseSsl": true,
		"Domain": "stage.example.com",
		"SearchBase": "dc=stage,dc=example,dc=com",
		"BindUsername": "stage-user",
		"BindPassword": "stage-password"
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*"
}